  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -wl --x64 --publish never"
  },
  "devDependencies": {
    "electron": "5.0.13",
    "electron-builder": "^22.9.1",
    "jest": "^26.2.2",
    "run-script-os": "^1.1.3"
  },
  "dependencies": {
    "electron-store": "^6.0.1",
    "fs": "0.0.1-security",
    "n-readlines": "^1.0.1",
    "path": "^0.12.7",
    "zeltice-mt-downloader": "^1.1.3"
  },
  "build": {
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "launcher.exe"
    }
  }
}
